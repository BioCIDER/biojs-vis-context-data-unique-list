  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/BioCIDER/biojs-vis-context-data-unique-list/blob/master/LICENSE"
    }
  ],
  "main": "js/index.js",
  "style": "css/*.css", 
  "scripts": {
		"test": "gulp test",
		"build": "gulp build",
		"build-install": "gulp build && npm run css_folder && npm run css",
		"build-browser": "gulp build-browser",
		"build-browser-min": "gulp build-browser-gzip",		
		"watch": "gulp watch",
		"test-watch": "gulp test-watch",
		"w": "prunner \"npm run sniper\" \"npm run watch\" \"npm run watch-css\"",
		"sniper": "sniper .",
		"css_folder": "mkdirp build/css",
		"css": "parcelify ./ -c build/css/bundle.css",
		"watch-css": "parcelify -w ./ -c build/css/bundle.css --loglevel verbose"
  },
  "dependencies": {
    "reqwest": "2.0.5"
    },
  "devDependencies": {
    "biojs-sniper": "^0.0.7",
    "browserify": "5.x",
    "del": "^0.1.3",
    "parcelify": "0.x",
    "chai": "^1.9.1",
    "gulp": "^3.8.8",	
    "gulp-chmod": "^1.1.1",
    "gulp-mocha": "^1.0.0",
    "gulp-gzip": "0.0.8",
    "gulp-rename": "^1.2.0",
    "gulp-streamify": "^0.0.5",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^1.0.2",
    "mkdirp": "^0.5.0",				
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^1.0.6"			
  }, 
  "sniper": {
    "js": [ "/build/biojsviscontextdatauniquelist.js"],
    "css": ["/build/css/bundle.css"], 
    "snippets": ["snippets"],
    "first": "biojs_example"
   }, 
  "keywords": [
    "biojs",
    "ELIXIR",
    "iAnn",
    "TeSS",
    "context"
  ]
}

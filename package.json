{
  "name": "biocider",
  "description": "Visualisation of ELIXIR training materials, events and registries in a unique list. ALPHA COMPONENT, TESTING PHASE.",
  "version": "0.5.12-alpha",
  "homepage": "https://github.com/BioCIDER/biojs-vis-context-data-unique-list",
  "author": {
    "name": "Carlos Horro",
    "email": "carlos.horro@tgac.ac.uk"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/BioCIDER/biojs-vis-context-data-unique-list.git"
  },
  "bugs": {
